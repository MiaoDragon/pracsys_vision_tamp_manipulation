<?xml version="1.0" ?>
<robot name="table.urdf">
  <link name="baseLink">
    <contact>
      <lateral_friction value="1.0"/>
    </contact>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
       <mass value=".0"/>
       <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
    <visual>
    <!-- Offset the x-axis by x-dim/2 so that we just need to specify the distance from robot base to table edge -->
      <origin rpy="0 0 0" xyz="0.381 0 0.4"/>
      <geometry>
			<mesh filename="table.obj" scale="0.762 1.215 0.8"/>
      </geometry>
       <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0.381 0 0.4"/>
      <geometry>
	 	<box size="0.762 1.215 0.8"/>
      </geometry>
    </collision>
  </link>
</robot>
